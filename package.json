{
  "name": "coffee-house",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "watch": "sass --watch sass/main.scss sass/style.css",
    "compile:sass": "sass sass/main.scss sass/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' sass/style.comp.css -o sass/style.prefix.css",
    "compress:css": "sass --style=compressed sass/style.prefix.css sass/style.css",
    "build:css": "npm-run-all compile:sass prefix:css compress:css"
  },
  "author": "Vitaly Kopochev",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.4.17",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^11.0.0",
    "sass": "^1.71.1"
  }
}
